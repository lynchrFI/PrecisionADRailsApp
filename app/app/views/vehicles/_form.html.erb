<%= form_with model: @vehicle, local: true do |f| %>

    <%= f.label :year %>
    <%= f.number_field :year %>
    <br>
    <br>
    <%= f.label :make %>
    <%= f.text_field :make%>
    <br>
    <br>
    <%= f.label :model%>
    <%= f.text_field :model %>
    <br>
    <br>

    <% end %>
    <%= f.submit %>

    <% end %>